{
  "name": "retrocode",
  "version": "1.2.0.0",
  "port-version": 0,
  "description": "RetroCode C++ utilities library",
  "homepage": "https://github.com/Retro-Technique/RetroCode",
  "dependencies": [
    {
      "name": "vcpkg-msbuild",
      "host": true
    }
  ],
  "features": {
    "collection": {
      "description": "Adding RetroCode-Collection - Containers library"
    },
    "exception": {
      "description": "Adding RetroCode-Exception - Exception library"
    },
    "image": {
      "description": "Adding RetroCode-Image - Image encoding and decoding library"
